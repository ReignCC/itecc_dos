// Ordinal Numbers by Reign C.

#include <stdio.h>

int main() {
 
    int range;
    printf("Enter number range: ");
    scanf("%d",&range);
    
    int modulo;
    for(int i = 1; i <= range; i++){
        printf("%d",i);
        modulo = i%10;
        
        if(modulo == 1 && i != 11){
            printf("st\n");
        }
        else if(modulo == 2 && i != 12){
            printf("nd\n");
        }
        else if(modulo == 3 && i != 13){
            printf("rd\n");
        }
        else{
            printf("th\n");
        }
    }
    
}
