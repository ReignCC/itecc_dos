#include <stdio.h>

int main(){
    
    int num, num1, a, b, c=0,i;
    
    printf("Enter the random number: ");
    scanf("%d", &num);
    num1=num;
    
    for(a=9;a>0;a--){
        for(i=num1;i>0;i/=10){
            b=i%10;
            if(b==a){
            c++; 
            }
    }
        if(c>1){
            break;
        }
        
    }
    printf("Greatest digit that occurred more than once = %d", a);
    
    return 0;
}
